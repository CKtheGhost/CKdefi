{
  "name": "compoundefi",
  "version": "1.0.0",
  "description": "AI-powered DeFi aggregator and yield optimizer for the Aptos blockchain",
  "main": "server/app.js",
  "scripts": {
    "dev": "nodemon server/app.js",
    "build:css": "tailwindcss -i ./client/src/styles/tailwind.css -o ./public/css/index.css --minify",
    "start": "node server/app.js",
    "build": "npm run build:css && cd client && react-scripts build"
  },
  "keywords": ["aptos", "defi", "staking", "dashboard", "crypto", "ai", "yield-optimizer", "portfolio-balancer"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.36.0",
    "anthropic": "^0.14.0",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "openai": "^4.24.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}