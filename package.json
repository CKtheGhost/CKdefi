    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.3",
    "openai": "^4.24.1",
