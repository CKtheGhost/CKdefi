{
  "name": "compoundefi",
  "version": "1.0.0",
  "description": "AI-powered DeFi aggregator and yield optimizer for Aptos blockchain",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "client": "cd client && npm start",
    "server": "nodemon server/index.js",
    "build": "cd client && npm run build",
    "dev:full": "concurrently \"npm run server\" \"npm run client\"",
    "install:all": "npm install && cd client && npm install",
    "test": "jest"
  },
  "keywords": [
    "defi",
    "aptos",
    "yield",
    "optimizer",
    "ai",
    "dashboard",
    "blockchain",
    "aggregator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.14.0",
    "@aptos-labs/ts-sdk": "^1.36.0",
    "@aptos-labs/wallet-adapter-react": "^2.1.4",
    "axios": "^1.6.7",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.8.6",
    "node-cache": "^5.1.2",
    "openai": "^4.24.1",
    "prom-client": "^14.2.0",
    "rate-limiter-flexible": "^6.2.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
